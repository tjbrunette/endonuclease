# Listing of default options for setup_cm.pl.
# Lines beginning with # characters are ignored.
boinc_tag           rosetta_cm
method              loopbuild_threading_cst_relax

# alignment filtering options
max_templates       20
max_pct_id          1.0
max_template_pct_id 1.0
max_e_value         10.0
# date cutoff of 0 means don't filter based on date at all
date_cutoff         0

#erase the following line to not use hhblits
#use_hhblits

# paths for external programs
mini_path           /work/tex/src/rosetta/rosetta_source/bin
mini_db_path        /work/tex/src/rosetta/rosetta_database
mini_compile_mode   release
mini_compiler       gcc

nr_db               /scratch/shared/genomes
hhsearch_path       /work/tex/hhsearch
